        <TextBox x:Name="resultTextBox" HorizontalAlignment="Left" Height="23" Margin="358,309,0,0" TextWrapping="Wrap" Text="" VerticalAlignment="Top" Width="120"/>
        <ListBox Name="listBox" HorizontalAlignment="Left" Height="23" Margin="358,140,0,0" VerticalAlignment="Top" Width="100">
