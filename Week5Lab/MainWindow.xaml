<Window x:Class="Week5Lab.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Week5Lab"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
    <Grid>
        <Canvas HorizontalAlignment="Left" Height="380" Name="TempCanvas" Margin="22,22,0,0" VerticalAlignment="Top" Width="743">
            <Label Content="Enter the speed limit:" Canvas.Left="120" Canvas.Top="69"/>
            <Label Content="Enter your speed:" Canvas.Left="140" Canvas.Top="118"/>
            <TextBox Name="speedLimitTextBox" Height="23" Canvas.Left="262" TextWrapping="Wrap" Text="" Canvas.Top="73" Width="120"/>
            <TextBox Name="speedTextBox" Height="23" Canvas.Left="262" TextWrapping="Wrap" Text="" Canvas.Top="122" Width="120"/>
            <Label  Name="messageLabel" Content="" Canvas.Left="223" Canvas.Top="235" Width="327"/>
            <Button Content="Click to Check Speed" Canvas.Left="289" Canvas.Top="174" Width="141" Click="Button_Click"/>
        </Canvas>

    </Grid>
</Window>
